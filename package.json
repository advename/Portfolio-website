{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Portfolio website",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass": "node-sass ./styles/sass/main.scss ./styles/style.css -w",
    "myParcel": "parcel index.html --public-url . -watch --no-minify",
    "dev-mode-server": "npm run sass | live-server --no-browser",
    "live-server-dist": "cd dist & live-server --no-browser",
    "dev-mode-parcel-combined": "npm run sass | npm run myParcel | npm run live-server-dist",
    "dev-mode-parcel-only": "parcel index.html",
    "build-mode-parcel": "parcel build index.html --public-url .",
    "clear": " cd dist & del *.* /q"
  },
  "author": "lars",
  "license": "ISC",
  "dependencies": {
    "gsap": "^2.0.2",
    "hammerjs": "^2.0.8",
    "is_js": "^0.9.0",
    "validator-js": "^0.2.1"
  },
  "devDependencies": {
    "cssnano": "^4.1.4",
    "node-sass": "^4.9.4",
    "parcel-plugin-static-files-copy": "^1.2.4",
    "sass": "^1.14.2"
  },
  "staticPath": "assets"
}
